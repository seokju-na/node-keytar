{
  "main": "./lib/keytar.js",
  "exports": {
    ".": "./lib/keytar.js"
  },
  "typings": "keytar.d.ts",
  "name": "@seokju-na/keytar",
  "description": "Bindings to native Mac/Linux/Windows password APIs",
  "version": "7.11.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/node-keytar.git"
  },
  "bugs": {
    "url": "https://github.com/atom/node-keytar/issues"
  },
  "homepage": "http://atom.github.io/node-keytar",
  "keywords": [
    "keychain",
    "password",
    "passwords",
    "credential",
    "credentials",
    "vault",
    "credential vault"
  ],
  "files": [
    "lib",
    "src",
    "binding.gyp",
    "keytar.d.ts"
